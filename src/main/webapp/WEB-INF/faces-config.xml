<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <navigation-rule>
        <description>
            Globale Navigationsregeln, gültig für alle Pageflows
        </description>
        
        <navigation-case>
            <from-outcome>home</from-outcome>
            <to-view-id>home.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>newAccount</from-outcome>
            <to-view-id>newAccount.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>login.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>bank</from-outcome>
            <to-view-id>bank.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>depositMoney</from-outcome>
            <to-view-id>depositMoney.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>moneyPayout</from-outcome>
            <to-view-id>moneyPayout.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>Account</from-outcome>
            <to-view-id>Account.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>transfer</from-outcome>
            <to-view-id>transfer.xhtml</to-view-id>
        </navigation-case>

        <navigation-case>
            <from-outcome>admin</from-outcome>
            <to-view-id>admin.xhtml</to-view-id>
        </navigation-case>

                
    </navigation-rule>
</faces-config>
